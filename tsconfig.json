{
    "compilerOptions": {
        "target": "es6",
        "lib": [
            "es2016",
            "dom"
        ],
        "module": "es6",
        "rootDir": "src",
        "declaration": true,
        "sourceMap": true,
        "outDir": "lib",
        "esModuleInterop": true,
        "moduleResolution": "Node",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}